<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>
	
	
	<!-- Alias (vo)설정 -->
	<typeAliases>
		 <typeAlias alias="member" type="com.goods.myapp.model.member.MemberVO" />
		 <typeAlias alias="productinfo" type="com.goods.myapp.model.product.ProductInfoVO" />
		 <typeAlias alias="goodspayment" type="com.goods.myapp.model.payment.GoodsPaymentVO" />
		 <typeAlias alias="basket" type="com.goods.myapp.model.basket.BasketVO" />
	</typeAliases> 
	
	<!-- Sql Mapper 설정 -->
	 <mappers>
		<mapper resource="mappers/member/member-mapping.xml" />
		<mapper resource="mappers/goods/productinfo-mapping.xml" />
		<mapper resource="mappers/payment/payment-mapping.xml" />
		<mapper resource="mappers/basket/basket-mapping.xml" />
	</mappers> 
</configuration>